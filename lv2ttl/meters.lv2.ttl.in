	] , [
		a lv2:AudioPort ,
			lv2:OutputPort ;
		lv2:index 5 ;
		lv2:symbol "out2" ;
		lv2:name "OutR" ;
	  rdfs:comment "signal pass-thru" ;
	] , [
		a lv2:OutputPort, lv2:ControlPort ;
		lv2:index 6 ;
		lv2:symbol "phase" ;
		lv2:name "phase" ;
