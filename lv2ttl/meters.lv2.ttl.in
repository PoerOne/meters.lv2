		lv2:minimum -40.0 ;
		lv2:maximum  40.0 ;
		units:unit units:db;
	  rdfs:comment "Display Signal Level Gain." ;
	] , [
		a lv2:ControlPort, lv2:InputPort ;
		lv2:index 8 ;
		lv2:symbol "cutoff" ;
		lv2:name "Cutoff" ;
		lv2:default -50.0 ;
		lv2:minimum -80.0 ;
		lv2:maximum -10.0 ;
		units:unit units:db;
